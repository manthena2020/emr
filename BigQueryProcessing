        "import pandas as pd\n",
        "from google.cloud import storage\n",
        "\n",
        "# Initialize a GCS client\n",
        "client = storage.Client()\n",
        "\n",
        "# Specify the GCS bucket and file path (without gs:// prefix)\n",
        "bucket_name = 'deeksha'\n",
        "file_path = 'ravi/depmaxsal.csv'\n",
        "\n",
        "# Create a reference to the GCS bucket and file\n",
        "bucket = client.get_bucket(bucket_name)\n",
        "blob = bucket.blob(file_path)\n",
        "\n",
        "# Download the CSV file to the /content directory\n",
        "temp_file = '/content/temp.csv'  # Use the appropriate path in your environment\n",
        "blob.download_to_filename(temp_file)\n",
        "\n",
        "# Read the CSV file into a Pandas DataFrame\n",
        "df = pd.read_csv(temp_file)\n",
        "\n",
        "# Print the DataFrame\n",
        "print(df.to_markdown(index=False))\n",
        "\n",
        "# You can now work with the 'df' DataFrame as needed"
      ],
